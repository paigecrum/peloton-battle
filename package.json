{
  "name": "peloton-battle-app",
  "version": "1.0.0",
  "description": "Peloton Battle App",
  "engines": {
    "node": "15.x"
  },
  "private": true,
  "scripts": {
    "start": "node ./api/index.js",
    "dev": "concurrently \"npm:start:server\" \"npm:start:client\"",
    "start:server": "nodemon ./api/index.js",
    "start:client": "cd ./ui && npm run dev",
    "heroku-postbuild": "cd ./ui && npm install && npm run build && cp -r ./build ../api/public"
  },
  "dependencies": {
    "concurrently": "^6.0.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
